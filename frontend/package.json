{
  "name": "frontend",
  "private": true,
  "type": "module",
  "main": "build/main.js",
  "description": "",
  "author": "",
  "version": "0.0.1",
  "build": {
    "productName": "CodeMachine",
    "win": {
      "target": "msi",
      "signAndEditExecutable": false,
      "icon": "public/code-machine-logo.ico"
    },
    "msi": {
      "oneClick": false,
      "runAfterFinish": false
    },
    "files": [
      "./build/**/*"
    ],
    "extraResources": [
      "module-electron/Accumulator_CPU_Chisel-assembly-0.1.0.jar"
    ]
  },
  "scripts": {
    "build": "react-router build --mode website",
    "build:electron": "react-router build --mode electron && vite build -c vite.config.electron.ts",
    "dev": "react-router dev --mode website",
    "dev:electron": "electron .",
    "package": "electron-builder",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "electron-is-dev": "^3.0.1",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
